class IOString(object):
    #nhan chuoi va in hoa
    def __init__(self):
        self.str1=""
    def get_String(self):
        self.str1=input("nhap 1 chuoi")
    def print_String(self):
        print(self.str1.upper())
str1 = IOString()
str1.get_String()
str1.print_String()